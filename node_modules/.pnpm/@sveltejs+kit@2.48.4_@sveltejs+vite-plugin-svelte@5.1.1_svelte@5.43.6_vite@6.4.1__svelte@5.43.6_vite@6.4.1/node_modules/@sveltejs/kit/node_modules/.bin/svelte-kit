#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tylerthorson/Atlas/node_modules/.pnpm/@sveltejs+kit@2.48.4_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.43.6_vite@6.4.1__svelte@5.43.6_vite@6.4.1/node_modules/@sveltejs/kit/node_modules:/Users/tylerthorson/Atlas/node_modules/.pnpm/@sveltejs+kit@2.48.4_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.43.6_vite@6.4.1__svelte@5.43.6_vite@6.4.1/node_modules/@sveltejs/node_modules:/Users/tylerthorson/Atlas/node_modules/.pnpm/@sveltejs+kit@2.48.4_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.43.6_vite@6.4.1__svelte@5.43.6_vite@6.4.1/node_modules:/Users/tylerthorson/Atlas/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tylerthorson/Atlas/node_modules/.pnpm/@sveltejs+kit@2.48.4_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.43.6_vite@6.4.1__svelte@5.43.6_vite@6.4.1/node_modules/@sveltejs/kit/node_modules:/Users/tylerthorson/Atlas/node_modules/.pnpm/@sveltejs+kit@2.48.4_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.43.6_vite@6.4.1__svelte@5.43.6_vite@6.4.1/node_modules/@sveltejs/node_modules:/Users/tylerthorson/Atlas/node_modules/.pnpm/@sveltejs+kit@2.48.4_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.43.6_vite@6.4.1__svelte@5.43.6_vite@6.4.1/node_modules:/Users/tylerthorson/Atlas/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../svelte-kit.js" "$@"
else
  exec node  "$basedir/../../svelte-kit.js" "$@"
fi
